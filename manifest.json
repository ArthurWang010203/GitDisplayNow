{
    "name": "GitDisplayNow",
    "description": "Displays the 3D representation of an obj file in github",
    "version": "0.0.0.1",
    "manifest_version": 3,
    "action": {
        "default_popup": "index.html"
    },
    "content_security_policy": {
        "extension_pages": "default-src 'self'"
    },
    "icons":{
        "128": "icon.png"
    },
    "permissions": ["activeTab","scripting"]
}